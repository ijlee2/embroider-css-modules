{
  "name": "ember-codemod-remove-ember-css-modules",
  "version": "0.1.4",
  "description": "Codemod to replace ember-css-modules with embroider-css-modules",
  "keywords": [
    "codemod",
    "css-modules",
    "ember-codemod",
    "ember-css-modules",
    "emberjs",
    "embroider-css-modules"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ijlee2/embroider-css-modules.git"
  },
  "license": "MIT",
  "author": "Isaac J. Lee",
  "main": "src/index.js",
  "type": "module",
  "bin": "./bin/ember-codemod-remove-ember-css-modules.js",
  "files": [
    "bin/",
    "src/"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "test": "mt tests --quiet"
  },
  "dependencies": {
    "@codemod-utils/ast": "^0.1.0",
    "@codemod-utils/blueprints": "^0.1.1",
    "@codemod-utils/files": "^0.2.0",
    "@codemod-utils/json": "^0.1.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@codemod-utils/tests": "^0.1.1",
    "@shared-configs/eslint-config": "workspace:*",
    "@shared-configs/prettier": "workspace:*",
    "@sondr3/minitest": "^0.1.1",
    "concurrently": "^8.1.0",
    "eslint": "^8.42.0",
    "prettier": "^2.8.8"
  },
  "engines": {
    "node": "16.* || >= 18"
  }
}
