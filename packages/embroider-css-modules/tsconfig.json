{
  "extends": "@shared-configs/typescript/ember",
  "compilerOptions": {
    "declarationDir": "declarations",
    "emitDeclarationOnly": true,
    "noEmit": false,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*",
    "unpublished-development-types/**/*"
  ],
  "glint": {
    "environment": "ember-loose"
  }
}
