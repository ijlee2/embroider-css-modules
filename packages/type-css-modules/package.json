{
  "name": "type-css-modules",
  "version": "0.1.2",
  "description": "Generate declaration files for CSS modules",
  "keywords": [
    "css",
    "css-modules",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ijlee2/embroider-css-modules.git"
  },
  "license": "MIT",
  "author": "Isaac J. Lee",
  "main": "src/index.js",
  "type": "module",
  "bin": "./bin/type-css-modules.js",
  "files": [
    "bin/",
    "src/"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "test": "mt tests --quiet"
  },
  "dependencies": {
    "@codemod-utils/files": "^0.1.0",
    "css-tree": "^2.3.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@codemod-utils/tests": "^0.1.0",
    "@shared-configs/eslint-config": "workspace:*",
    "@shared-configs/prettier": "workspace:*",
    "@sondr3/minitest": "^0.1.1",
    "concurrently": "^8.0.1",
    "eslint": "^8.42.0",
    "prettier": "^2.8.8"
  },
  "engines": {
    "node": "16.* || >= 18"
  }
}
