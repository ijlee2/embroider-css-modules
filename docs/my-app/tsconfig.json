{
  "extends": "@shared-configs/typescript/ember",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "my-app/tests/*": [
        "tests/*"
      ],
      "my-app/*": [
        "app/*"
      ],
      "*": [
        "types/*"
      ]
    },
    "skipLibCheck": true
  },
  "glint": {
    "environment": [
      "ember-loose",
      "ember-template-imports"
    ]
  }
}
