{
  "name": "sample-v2-addon",
  "version": "0.1.0",
  "private": true,
  "description": "Sample v2 addon for docs-app",
  "keywords": [
    "css",
    "css-modules",
    "ember-addon",
    "emberjs",
    "embroider",
    "glint",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ijlee2/embroider-css-modules.git"
  },
  "license": "MIT",
  "author": "Isaac J. Lee",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./addon-main.js": "./addon-main.cjs"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "files": [
    "addon-main.cjs",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\" --cache",
    "lint:css:fix": "stylelint \"**/*.css\" --fix",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "prepack": "rollup --config",
    "start": "rollup --config --watch",
    "test": "echo 'A v2 addon does not have tests, run tests in tests/sample-v2-addon'"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.8.4",
    "embroider-css-modules": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@embroider/addon-dev": "^3.0.0",
    "@glimmer/component": "^1.1.2",
    "@glint/core": "^1.0.1",
    "@glint/environment-ember-loose": "^1.0.1",
    "@glint/environment-ember-template-imports": "^1.0.1",
    "@glint/template": "^1.0.1",
    "@rollup/plugin-babel": "^6.0.3",
    "@tsconfig/ember": "^2.0.0",
    "concurrently": "^8.0.1",
    "ember-template-lint": "^5.7.2",
    "ember-template-lint-plugin-prettier": "^4.1.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-ember": "^11.5.2",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^2.8.8",
    "rollup": "^3.21.0",
    "stylelint": "^15.6.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-no-unsupported-browser-features": "^6.1.0",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^3.0.0"
  },
  "engines": {
    "node": "16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "app-js": {
      "./components/hello-world.js": "./dist/_app_/components/hello-world.js"
    },
    "main": "addon-main.cjs",
    "type": "addon",
    "version": 2
  }
}
