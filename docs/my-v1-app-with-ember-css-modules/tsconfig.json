{
  "extends": "@shared-configs/typescript/ember",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "my-v1-app-with-ember-css-modules/tests/*": [
        "tests/*"
      ],
      "my-v1-app-with-ember-css-modules/*": [
        "app/*"
      ],
      "*": [
        "types/*"
      ]
    },
    "skipLibCheck": true,
    "types": [
      "ember-source/types"
    ]
  },
  "glint": {
    "environment": [
      "ember-loose",
      "ember-template-imports"
    ]
  }
}
