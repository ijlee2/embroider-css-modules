{
  "name": "my-v1-app-with-ember-css-modules",
  "version": "3.0.0",
  "private": true,
  "description": "An Ember app with ember-css-modules",
  "keywords": [
    "css",
    "css-modules",
    "emberjs",
    "embroider",
    "glint",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ijlee2/embroider-css-modules.git"
  },
  "license": "MIT",
  "author": "Isaac J. Lee",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "_build": "ember build --environment=production",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"app/**/*.css\" --cache",
    "lint:css:fix": "stylelint \"app/**/*.css\" --fix",
    "lint:dependency": "ember dependency-lint",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "prelint:types": "type-css-modules --src app",
    "lint:types": "glint",
    "start": "ember serve",
    "test": "ember test --test-port=0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@ember/optional-features": "^2.2.0",
    "@ember/string": "^4.0.1",
    "@ember/test-helpers": "^5.2.1",
    "@embroider/broccoli-side-watch": "^1.1.0",
    "@embroider/compat": "^3.9.0",
    "@embroider/core": "^3.5.6",
    "@embroider/router": "^2.1.8",
    "@embroider/webpack": "^4.1.0",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "@glint/core": "^1.5.2",
    "@glint/environment-ember-loose": "^1.5.2",
    "@glint/environment-ember-template-imports": "^1.5.2",
    "@glint/template": "^1.5.2",
    "@percy/cli": "^1.30.10",
    "@percy/ember": "^4.2.0",
    "@shared-configs/ember-template-lint": "workspace:*",
    "@shared-configs/eslint-config-ember": "workspace:*",
    "@shared-configs/prettier": "workspace:*",
    "@shared-configs/stylelint": "workspace:*",
    "@shared-configs/testem": "workspace:*",
    "@shared-configs/typescript": "workspace:*",
    "@types/qunit": "^2.19.12",
    "autoprefixer": "^10.4.21",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^9.1.2",
    "ember-a11y-testing": "^7.1.2",
    "ember-auto-import": "^2.10.0",
    "ember-cli": "~6.4.0",
    "ember-cli-app-version": "^7.0.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.3",
    "ember-cli-dependency-lint": "^2.0.1",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-css-modules": "^2.1.1",
    "ember-load-initializers": "^3.0.1",
    "ember-page-title": "^9.0.1",
    "ember-qunit": "^9.0.2",
    "ember-resolver": "^13.1.0",
    "ember-source": "~6.4.0",
    "ember-template-imports": "^4.3.0",
    "ember-template-lint": "^7.6.0",
    "ember-test-selectors": "^7.1.0",
    "embroider-css-modules": "workspace:*",
    "eslint": "^9.26.0",
    "loader.js": "^4.7.0",
    "my-v2-addon": "workspace:*",
    "prettier": "^3.5.3",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.4.0",
    "stylelint": "^16.19.1",
    "type-css-modules": "workspace:*",
    "typescript": "^5.8.3",
    "webpack": "^5.99.8"
  },
  "engines": {
    "node": "20.* || >= 22"
  },
  "ember": {
    "edition": "octane"
  }
}
